{
  "name": "stylelint-config-neat",
  "version": "1.0.0",
  "description": "A neat shareable config for stylelint",
  "main": "index.js",
  "files": [
    "index.js",
    "properties-order.js"
  ],
  "scripts": {
    "lint:formatting": "prettier --check .",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:md": "remark --quiet --frail --ignore-path .gitignore .",
    "lint": "npm-run-all --parallel lint:*",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended",
      "preset-prettier"
    ]
  },
  "lint-staged": {
    "*.{js,json,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlestati/stylelint-config-neat.git"
  },
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "author": "Charles TatibouÃ«t",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/charlestati/stylelint-config-neat/issues"
  },
  "homepage": "https://github.com/charlestati/stylelint-config-neat#readme",
  "dependencies": {
    "stylelint-config-recommended": "6.0.0",
    "stylelint-no-unsupported-browser-features": "5.0.2",
    "stylelint-order": "5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.8",
    "@semantic-release/release-notes-generator": "9.0.1",
    "eslint": "8.6.0",
    "eslint-config-stylelint": "15.0.0",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "lint-staged": "12.1.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "remark-cli": "9.0.0",
    "remark-preset-lint-recommended": "5.0.0",
    "remark-preset-prettier": "0.4.0",
    "semantic-release": "17.2.3",
    "stylelint": "14.2.0"
  }
}
